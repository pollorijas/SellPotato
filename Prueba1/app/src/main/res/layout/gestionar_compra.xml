<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/Prueba1"
    android:clickable="false"
    android:background="#ffffffff">


    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="91dp"
            android:layout_gravity="right|top">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView3"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:src="@drawable/agro3" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="393dp"
            android:layout_gravity="right|bottom">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imgproductoadd"
                android:src="@drawable/gp"
                android:layout_marginTop="23dp"
                android:layout_alignParentTop="true"
                android:layout_alignLeft="@+id/textView5"
                android:layout_alignStart="@+id/textView5" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imgproductoupd"
                android:layout_alignBottom="@+id/imgproductoadd"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="42dp"
                android:layout_marginEnd="42dp"
                android:src="@drawable/cp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Generar Pedido"
                android:id="@+id/textView2"
                android:layout_below="@+id/imgproductoadd"
                android:layout_toLeftOf="@+id/textView3"
                android:layout_toStartOf="@+id/textView3"
                android:layout_marginRight="27dp"
                android:layout_marginEnd="27dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Cancelar Pedido"
                android:id="@+id/textView3"
                android:layout_above="@+id/imageButton4"
                android:layout_alignRight="@+id/textView28"
                android:layout_alignEnd="@+id/textView28" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton4"
                android:src="@drawable/mp"
                android:layout_below="@+id/textView2"
                android:layout_alignLeft="@+id/imgproductoadd"
                android:layout_alignStart="@+id/imgproductoadd"
                android:layout_marginTop="19dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton5"
                android:layout_alignRight="@+id/imgproductoupd"
                android:layout_alignEnd="@+id/imgproductoupd"
                android:src="@drawable/ep"
                android:layout_alignBottom="@+id/imageButton4" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Modificar Pedido"
                android:id="@+id/textView5"
                android:layout_below="@+id/imageButton5"
                android:layout_alignLeft="@+id/textView2"
                android:layout_alignStart="@+id/textView2"
                android:layout_marginLeft="7dp"
                android:layout_marginStart="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Estado Pedido"
                android:id="@+id/textView28"
                android:layout_below="@+id/imageButton4"
                android:layout_alignLeft="@+id/imgproductoupd"
                android:layout_alignStart="@+id/imgproductoupd" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imgpago"
                android:src="@drawable/pp"
                android:layout_below="@+id/textView5"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="18dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Realizar Pago"
                android:id="@+id/textView29"
                android:layout_below="@+id/imgpago"
                android:layout_centerHorizontal="true" />

        </RelativeLayout>

    </FrameLayout>
</RelativeLayout>
