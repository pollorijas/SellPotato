<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/Prueba1"
    android:clickable="false"
    android:background="#ffffffff">


    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="91dp"
            android:layout_gravity="right|top">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView3"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:src="@drawable/agro3" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="393dp"
            android:layout_gravity="right|bottom"
            android:id="@+id/ly2">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imgwp"
                android:src="@drawable/camion"
                android:layout_marginLeft="36dp"
                android:layout_marginStart="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imgpce"
                android:src="@drawable/mapa"
                android:layout_centerVertical="true"
                android:layout_alignLeft="@+id/imgwp"
                android:layout_alignStart="@+id/imgwp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton7"
                android:src="@drawable/pp"
                android:layout_alignBottom="@+id/imgpce"
                android:layout_alignRight="@+id/textView21"
                android:layout_alignEnd="@+id/textView21" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imgordenar"
                android:src="@drawable/ordenar"
                android:layout_marginRight="40dp"
                android:layout_marginEnd="40dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Gestionar Camión"
                android:id="@+id/textView20"
                android:layout_below="@+id/imgwp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="18dp"
                android:layout_marginStart="18dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Cargar Camión"
                android:id="@+id/textView21"
                android:layout_below="@+id/imgordenar"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="29dp"
                android:layout_marginEnd="29dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Generar Recorrido"
                android:id="@+id/textView22"
                android:layout_below="@+id/imgpce"
                android:layout_alignRight="@+id/textView20"
                android:layout_alignEnd="@+id/textView20"
                android:layout_marginRight="7dp"
                android:layout_marginEnd="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Realizar Cobro"
                android:id="@+id/textView23"
                android:layout_alignTop="@+id/textView22"
                android:layout_alignLeft="@+id/imgordenar"
                android:layout_alignStart="@+id/imgordenar" />
        </RelativeLayout>

    </FrameLayout>
</RelativeLayout>
